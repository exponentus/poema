{
  "name": "nb-ng",
  "version": "1.7.0",
  "author": "LOF",
  "license": "LOF",
  "description": "nb front-end",
  "scripts": {
    "clean:js": "rimraf ../assets/dist",
    "clean:aot": "rimraf compiled",
    "copy-config": "copyfiles -f ./node_modules/@nb/core/config/auto-configure.js ./",
    "build": "npm run clean:js && webpack --config config/webpack.config.js --mode development",
    "build:w": "npm run clean:js && webpack --config config/webpack.config.js --watch --mode development",
    "build:aot:p": "npm run clean:js && npm run clean:aot && webpack --config config/webpack.config.js --mode production --optimize-minimize",
    "lint": "tslint ./src/**/*.ts{,x}",
    "explorer": "source-map-explorer ../assets/dist/app.js ../assets/dist/app.js.map",
    "explorer:vendor": "source-map-explorer ../assets/dist/vendor.js ../assets/dist/vendor.js.map",
    "reinstall-nb-core": "npm rm @nb/core && npm i git+ssh://git@git.flabser.com:12227/b2b/nb-ng.git --save && npm run postinstall",
    "reinstall-nb-core:build:aot:p": "npm rm @nb/core && npm i git+ssh://git@git.flabser.com:12227/b2b/nb-ng.git --save && npm run postinstall && npm run build:aot:p",
    "postinstall": "npm run copy-config && node auto-configure.js"
  },
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "@nb/core": "git+ssh://git@git.flabser.com:12227/b2b/nb-ng.git",
    "@ngx-translate/core": "11.0.1",
    "angular-calendar": "0.26.4",
    "chart.js": "2.7.3",
    "core-js": "2.5.7",
    "date-fns": "1.29.0",
    "file-saver": "2.0.0",
    "marked": "0.5.2",
    "moment": "2.22.2",
    "pikaday-time": "1.6.1",
    "rxjs": "6.3.3",
    "turndown": "5.0.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.0",
    "@angular/cli": "7.1.0",
    "@angular/compiler-cli": "7.1.1",
    "@ngtools/webpack": "7.1.0",
    "@types/core-js": "2.5.0",
    "@types/node": "10.12.0",
    "@types/uglify-js": "3.0.4",
    "circular-dependency-plugin": "5.0.2",
    "codelyzer": "4.5.0",
    "compression-webpack-plugin": "2.0.0",
    "copyfiles": "2.1.0",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "html-loader": "0.5.5",
    "mini-css-extract-plugin": "0.4.5",
    "ngc-webpack": "4.1.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "source-map-explorer": "1.6.0",
    "style-loader": "0.23.1",
    "to-string-loader": "1.1.5",
    "tslint": "5.11.0",
    "typescript": "3.1.3",
    "uglifyjs-webpack-plugin": "1.3.0",
    "webpack": "4.26.1",
    "webpack-command": "0.4.2"
  }
}
