{
  "name": "poema-projects",
  "version": "1.0.0",
  "author": "LOF/MK",
  "license": "LOF",
  "description": "Task manager",
  "scripts": {
    "postinstall": "npm run typings install",
    "typings": "typings",
    "start": "node server.js",
    "clean": "rm -rf js",
    "build": "npm run clean && webpack --config config/webpack.config.js",
    "build:w": "npm run clean && webpack -w --config config/webpack.config.js",
    "build:p": "npm run clean && webpack --config config/webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "minifyjs": "uglifyjs js/app.js --screw-ie8 --compress --mangle --output js/app.min.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "^2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.1.2",
    "marked": "0.3.5",
    "moment": "2.14.1",
    "ng2-translate": "^2.4.3",
    "pikaday": "1.4.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "to-markdown": "3.0.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "angular2-router-loader": "^0.3.0",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-es2015-webpack": "^6.4.3",
    "compression-webpack-plugin": "^0.3.1",
    "css-loader": "^0.24.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "ngrx-store-logger": "^0.1.7",
    "raw-loader": "^0.5.1",
    "to-string-loader": "^1.1.4",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "uglifyjs": "^2.4.10",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.4"
  }
}
